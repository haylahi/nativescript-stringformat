<Page xmlns="http://schemas.nativescript.org/tns.xsd" navigatingTo="onNavigatingTo">
  <Page.actionBar>
    <ActionBar title="nativescript-stringformat Demo">
    </ActionBar>
  </Page.actionBar>
  
  <ListView items="{{ items }}">
    <ListView.itemTemplate>
      <StackLayout>
        <Label text="{{ description }}" />
        <Label text="{{ result }}" />
      </StackLayout>
    </ListView.itemTemplate>
  </ListView>
</Page>